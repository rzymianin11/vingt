<div class="py-5 mx-0 px-0">
	<div class="row no-gutters  my-5">
		<div class="col-12" style="
			{% if data.inner_padding1 != blank %} 
				padding: {{ data.inner_padding1 }};
			{% endif %}
			{% if data.bg-color1 != blank %} 
				background-color: {{ data.bg-color1 }};
			{% endif %}
				">
			<div class="w-100" style="position: relative;">
				<img style="display: block;" src="{% include 'img_url' with url: data.6[0].url, params: '1920x' %}" class="w-100" />
				{% if data.6[0].title != '' %} 
					<div class="label" style="position: absolute; bottom: -2.5em; right: 20px; color: white; font-size: 15px; letter-spacing: 0.16px; font-weight: 100;">
						{{ data.6[0].title }}
					</div>
				{% endif %}
			</div>
		</div>
	</div>
</div>